import random

chars = "abcdefghijklmnopqrstuvxwyzABCDEFGHIJKLMNOPQRSTUVXWYZ12345¨67890!@#$%¨&*()<>:;?[]{}"

while 1:
    password_len = int(input("Qual o comprimento de sua senha: "))
    password = ""
    for w in range(0 , password_len):
        password_char = random.choice(chars)
        password = password + password_char
    print("Sua senha: ", password)